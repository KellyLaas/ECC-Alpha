<div class="panel panel-default">
    <div class="panel-heading">
      <div class="row">
        <div class="col-md-12">
          <h4 class="pull-left"> <%= @code.code_title %> [<%= @code.code_year %>] </h4>
          <h4 class="pull-left"><%= link_to @org.name, {controller: "orgs", action: :show, id: @org.id } %></h4> 
        </div>
      </div>
    </div>
    <div class="panel-body">
      <%= raw(@code.code_content) %>
    </div>
</div>
