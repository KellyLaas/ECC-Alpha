<div class="panel panel-default">
    <div class="panel-heading">
      <div class="row">
        <div class="col-md-12">
          <h4 class="pull-left"> <%= @org.name %> </h4>
        </div>
      </div>
    </div>
    <div class="panel-body">
      <p> <%= raw(@org.desc) %> </p>
      <div class="code-body">
        <% if @codes.empty %>
          <h3> Organization has yet to add codes! </h3>
        <% else %>
          <% for each @codes do |code| %>
        <% end %>
      </div>
    </div>
</div>
