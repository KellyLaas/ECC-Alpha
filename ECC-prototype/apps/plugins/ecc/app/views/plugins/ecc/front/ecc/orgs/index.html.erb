<%#= javascript_include_tag plugin_gem_asset("js/my_file.js") %>

<h2> Welcome to the Ethics Codes Collection Site!</h2>
<div class="panel panel-default">
  <div class="panel-heading">
    <div class="row">
      <div class="col-md-12">
      </div>
    </div>
</div> 
<div class="panel-body">
  <% if @orgs.empty? %>
    <h4> No Organizationss To Display! </h4>
  <% else %>
    <table class="table table-responsive table-hover table-striped">
      <thead>
        <th> Organization </th>
        <th> Link  </th>
      </thead>
      <tbody>
        <% @orgs.each do |org| %>
        <tr>
          <td><%= link_to org.name, {action: :show, id: org.id} %></td>
          <td>Place holder for links to orgs</td>
        </tr>
        <% end %>
      </tbody>
      </table>
  <% end %>
  <%= will_paginate @orgs %>
  </div>
</div>             
